#include<iostream>
#include <windows.h>
#include <stdio.h>
using namespace std;
int main()
{
	SYSTEMTIME st;
	GetLocalTime(&st);
	cout << st.wYear << "-" << st.wMonth << "-" << st.wDay << " ";
	cout << st.wHour << ":" << st.wMinute << ":" << st.wSecond << "." << st.wMilliseconds << endl;
	system("pause");
	return 0;
}
